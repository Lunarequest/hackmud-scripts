{
  "compilerOptions": {
    "target": "es6",
    "module": "ES2015",
    "outDir": "dist",
    "moduleResolution": "node",
    "lib": ["es7"],
    "declaration": false,
    "sourceMap": false,
    "removeComments": false,
    "isolatedModules": false,
    "allowJs": true,
    "strict": false,
    "noImplicitAny": false,
    "skipLibCheck": true
  },
  "exclude": [ "node_modules", "dist", ".yarn", "tools" ]
}